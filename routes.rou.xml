<routes>
    <!-- Vehicle type -->
    <vType id="car"
           accel="2.6"
           decel="4.5"
           maxSpeed="13.9"
           length="5"/>

    <!-- ROUTES -->

    <!-- Bottom to top -->
    <route id="r0" edges="-E3 A0A1 E6"/>
    <route id="r1" edges="-E2 B0B1 E0"/>

    <!-- Left to right -->
    <route id="r2" edges="-E5 A0B0 E4"/>
    <route id="r3" edges="-E7 A1B1 E1"/>

    <!-- Cross-grid -->
    <route id="r4" edges="-E3 A0B0 B0B1 E0"/>
    <route id="r5" edges="-E6 A1A0 A0B0 E4"/>

    <!-- FLOWS from dead ends -->
    <flow id="flow0" type="car" route="r0" begin="0" end="300" vehsPerHour="600"/>
    <flow id="flow1" type="car" route="r1" begin="0" end="300" vehsPerHour="600"/>
    <flow id="flow2" type="car" route="r2" begin="0" end="300" vehsPerHour="600"/>
    <flow id="flow3" type="car" route="r3" begin="0" end="300" vehsPerHour="600"/>
    <flow id="flow4" type="car" route="r4" begin="0" end="300" vehsPerHour="600"/>
    <flow id="flow5" type="car" route="r5" begin="0" end="300" vehsPerHour="600"/>
</routes>
