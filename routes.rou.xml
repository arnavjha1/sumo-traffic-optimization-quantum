<routes>
    <!-- Vehicle type -->
    <vType id="car"
           accel="2.6"
           decel="4.5"
           maxSpeed="13.9"
           length="5"/>

    <!-- FLOWS from network entry edges -->
    <!-- Cars enter the network from edges but their turns are probabilistic -->
    <flow id="flow0" type="car" from="-E3" begin="0" end="300" vehsPerHour="600"/>
    <flow id="flow1" type="car" from="-E2" begin="0" end="300" vehsPerHour="600"/>
    <flow id="flow2" type="car" from="-E5" begin="0" end="300" vehsPerHour="600"/>
    <flow id="flow3" type="car" from="-E7" begin="0" end="300" vehsPerHour="600"/>
    <flow id="flow4" type="car" from="-E6" begin="0" end="300" vehsPerHour="600"/>
    <flow id="flow5" type="car" from="-E0" begin="0" end="300" vehsPerHour="600"/>
</routes>
